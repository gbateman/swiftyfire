swiftc StringUtils.swift JSONNodes.swift Tokenizer.swift Parser.swift Swiftifier.swift main.swift -o linux-SwiftyFire

mkdir linux-libs

cp /usr/local/swift/usr/lib/swift/linux/*.so linux-libs/
cp /usr/lib/x86_64-linux-gnu/libicudata.so.52 linux-libs/
cp /usr/lib/x86_64-linux-gnu/libicui18n.so.52 linux-libs/
cp /usr/lib/x86_64-linux-gnu/libicuuc.so.52 linux-libs/
cp /usr/lib/x86_64-linux-gnu/libbsd.so linux-libs/libbsd.so.0
