swiftc -L -lFoundation StringUtils.swift JSONNodes.swift Tokenizer.swift Parser.swift Swiftifier.swift main.swift -o linux-SwiftyFire

mkdir -p linux-libs/

cp /usr/lib/swift/linux/*.so linux-libs/
cp /usr/lib/x86_64-linux-gnu/libicudata.so.55 linux-libs/
cp /usr/lib/x86_64-linux-gnu/libicui18n.so.55 linux-libs/
cp /usr/lib/x86_64-linux-gnu/libicuuc.so.55 linux-libs/
cp /usr/lib/x86_64-linux-gnu/libbsd.so linux-libs/libbsd.so.0
